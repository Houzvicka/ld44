//player
can_animate = true

function make_player()
	player = {}
	player.x = 10
	player.y = 10
	player.dy = 0
	player.lives = 0
	player.ncoin = 0
	player.sprite = 64
end

function animate_player()
	if can_animate then
		player.sprite=fs[fp]
	end
end

function move_player()

 on_ground=false
	player.dy = gravity*2
	
	--ground colision
 if hit(player.x,player.y+player.dy,30,30) == 8 then
  on_ground=true
  player.dy=0
  else
  on_ground=false      
 end
	
	--jump
 if (btnp(2)) then
  if (on_ground) then
   player.dy-=11
  end
 end
 
 --move player right
 if btn(1) then
   player.x+=1 --right
 end
 
 --move player left
 if btn(0) then
   player.x-=1 --left
   can_animate = false
 end
 
 --move to new position
 player.y+=player.dy
 player.x+=1
 animate_player()
end

function draw_player()
	spr(player.sprite,player.x,player.y,4,4)
end
