//main
coins = {}

function _init()
 game_over=false
 make_player()

 for i=0,3 do
		newcoin = make_coin(i)
		add(coins,newcoin)
 end
 
end

function _update()
 move_player()
 
 for c in all(coins) do
 	move_coin(c)
 	check_collision(c)
 	animate_coin(c)
 end
 
end

function _draw()
 cls()
 map(0, 0, 0, 0, 128, 32)
 draw_player()
 for c in all(coins) do
 	draw_coin(c)
 end 
end

function wait(a) 
	for i = 1,a do 
		flip() 
	end 
end
