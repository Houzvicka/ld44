//main

function _init()
 game_over=false
 make_player()
 make_coin()
end

function _update()
 move_player()
 move_coin()
 check_collision()
 animate_coin()
end

function _draw()
 cls()
 map(0, 0, 0, 0, 128, 32)
 draw_player()
 draw_coin()
end

function wait(a) 
	for i = 1,a do 
		flip() 
	end 
end
