//coin

function make_coin()
	coin = {}
	coin.x = 0
	coin.y = 0
	coin.dy = 1
	coin.value = 0
	coin.sprite = 1
end

function animate_coin()
	coin.sprite+=1
 if (coin.sprite==6) coin.sprite=1
	wait(3)
end

function move_coin()
	gravity=1
	coin.dy = gravity
	coin.y += coin.dy	
end

function check_collision()
